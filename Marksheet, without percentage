#include <iostream>

using namespace std;

int main()
{
    char name[12];
    int Totalmarks,obtainedmarks,grade,rollnumber,sum,num1,num2,num3;
    string pass, fail;
    cout<<"******************RESULT_COMPILER****************\n\n";
    cout<<"___________________Student's Status_____________:)\n";
    
    // Student's personal information;
    cout<<"Name of student\n";
