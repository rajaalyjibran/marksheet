    cout<<"Student's current grade\n";
    cin>>grade;
    
   cout<<"********MARKS IN ALL SUBJECTS*********\n\n";
    // Subjectwise Marks
    cout<<"Marks in English (out of 100) ___";
    cin>>num1;
    cout<<"Marks in Urdu (out of 100) ___";
    cin>>num2;
    cout<<"Marks in Mathematics (out of 100) ___ ";
    cin>>num3;
   sum=num3+num2+num1 ;
   
    cout<<"OBTAINED MARKS "<<sum ;
    
    //result
    if(obtainedmarks=sum>=140)
    {
      cout<<"\nYou are _____PASS";
      
    }
    
    else
    {
        cout<<"You are______FAIL";
       
    }
    
    return 0;
}
